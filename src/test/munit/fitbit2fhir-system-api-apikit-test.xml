            <dw:set-variable variableName="id"><![CDATA[%dw 1.0
%output application/java
---
"123"]]></dw:set-variable>
            <dw:set-variable variableName="fitbitClientId"><![CDATA[%dw 1.0
%output application/java
---
p('fitbit.client.id')]]></dw:set-variable>
